{
  "compilerOptions": {
    "noEmit": true,
    "strict": true,
    "target": "es6",
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "baseUrl": "./",
    "paths": {
      "@hooks/*": ["src/hooks/*"],
      "@store/*": ["src/store/*"],
      "@src/*": ["src/*"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
